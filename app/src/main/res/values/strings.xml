	<!-- Dialog Messages -->
	<string name="dialog_enable_keyword_blocking_title">Enable Keyword Blocking</string>
	<string name="dialog_enable_keyword_blocking_message">To block inappropriate keywords in apps, you need to enable the accessibility service:\n\nüîç What it does:\n‚Ä¢ Monitors text in apps for blocked keywords\n‚Ä¢ Blocks inappropriate content automatically\n‚Ä¢ Works completely offline on your device\n\nüì± How to enable:\n1. Tap \'Enable Now\' below\n2. Find \'InternetGuard Pro\' in the list\n3. Toggle the switch ON\n4. Return to this app\n\nüõ°Ô∏è Your privacy is protected - no data leaves your device\nThis is a one-time setup for your security.</string>
	<string name="dialog_permission_required_title">Permission Required</string>
	<string name="dialog_permission_required_message">To add and use keywords for blocking, you need to enable the accessibility service first.\n\nThis allows the app to monitor text in other apps and block inappropriate content.\n\nüõ°Ô∏è Your privacy is protected - all processing happens locally on your device.</string>
	<string name="dialog_help_keyword_blocking_title">Help - Keyword Blocking</string>
	<string name="dialog_help_keyword_blocking_message">How to use keyword blocking:\n\n1. Enable Accessibility Service first\n2. Add keywords you want to block\n3. Assign keywords to specific apps\n4. Test API connection if needed\n\nThe service will automatically block inappropriate content when detected.\n\nüõ°Ô∏è All processing happens locally on your device for maximum privacy.</string>
	<string name="dialog_choose_language_title">Choose Language</string>
	<string name="dialog_privacy_settings_title">Privacy Settings</string>
	<string name="dialog_privacy_settings_message">InternetGuard Pro respects your privacy. All data is stored locally and never shared with third parties.</string>
	<string name="dialog_about_title">About InternetGuard Pro</string>
	<string name="dialog_about_message">Version 1.0.0\n\nA powerful internet guard app with AI-powered suggestions and advanced blocking features.</string>
	<string name="dialog_cloud_ai_moderation_title">Cloud AI Moderation</string>
	<string name="dialog_clear_all_data_title">Clear All Data</string>
	<string name="dialog_clear_all_data_message">This will permanently delete all your data including rules, keywords, and statistics. This action cannot be undone.</string>
	<string name="dialog_service_enabled_title">‚úÖ Keyword Blocking Active</string>
	<string name="dialog_service_enabled_message">The accessibility service is enabled and working properly.\n\nYou can now add keywords and they will be automatically blocked in other apps.</string>
	<string name="dialog_service_enabled_success_title">üéâ Service Enabled!</string>
	<string name="dialog_service_enabled_success_message">Great! Keyword blocking is now active and will protect you from inappropriate content in apps.</string>
	
